//max, min and their diff
#include<stdio.h>
#include<conio.h>
void main()
{
	int i,n,a[n],max,min,diff;
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
	scanf("%d",&a[i]);
    }
    printf("\n");
    max=a[0];
    for(i=0;i<n;i++)
    {
    	if(a[i]>max)
    	max=a[i];
	}
	printf("%d\n",max);
	min=a[0];
	for(i=0;i<n;i++)
	{
		if(a[i]<min)
		min=a[i];
	}
	printf("%d\n",min);
	diff=max-min;
	printf("%d",diff);
}
